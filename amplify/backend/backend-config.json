{
    "function": {
        "userpoolPreSignup": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda"
        }
    },
    "auth": {
        "userpool": {
            "service": "Cognito",
            "providerPlugin": "awscloudformation",
            "dependsOn": [
                {
                    "category": "function",
                    "resourceName": "userpoolPreSignup",
                    "triggerProvider": "Cognito",
                    "attributes": [
                        "Arn",
                        "Name"
                    ]
                }
            ]
        }
    },
    "api": {
        "satellitedashboard": {
            "service": "AppSync",
            "providerPlugin": "awscloudformation",
            "output": {
                "authConfig": {
                    "additionalAuthenticationProviders": [
                        {
                            "authenticationType": "AMAZON_COGNITO_USER_POOLS",
                            "userPoolConfig": {
                                "userPoolId": "authuserpool"
                            }
                        },
                        {
                            "authenticationType": "AWS_IAM"
                        }
                    ],
                    "defaultAuthentication": {
                        "authenticationType": "API_KEY",
                        "apiKeyConfig": {
                            "description": "public",
                            "apiKeyExpirationDays": "365"
                        }
                    }
                }
            }
        }
    },
    "storage": {
        "media": {
            "service": "S3",
            "providerPlugin": "awscloudformation"
        }
    }
}